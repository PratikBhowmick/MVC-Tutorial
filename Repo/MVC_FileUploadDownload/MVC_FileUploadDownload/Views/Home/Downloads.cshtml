@model List<string>
@{
    ViewBag.Title = "Downloads";
}
<h2>Downloads</h2>
<table border="1">
    <tr>
        <th>File Name</th>
        <th>&nbsp;&nbsp;</th>
        <th>Link</th>
    </tr>
    @for (var i = 0; i <= Model.Count - 1; i++)
    {
        <tr>
            <td>@Model[i].ToString() </td>
            <td>&nbsp;&nbsp;</td>
            <td>@Html.ActionLink("Download", "Download", new { ImageName = @Model[i].ToString() }) </td>
        </tr>
    }
</table>  