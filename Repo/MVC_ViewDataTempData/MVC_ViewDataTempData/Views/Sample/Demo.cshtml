
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewBag</title>
</head>
<body>
    <div> 
        @foreach(string country in ViewData["Countries"] as List<string>)
        {
            <ul>
                <li>@country</li>
            </ul>
        }
    </div>
    <p>Current : @ViewBag.CurrentDate</p>
</body>
</html>
